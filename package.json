{
  "name": "chatty",
  "productName": "Chatty",
  "version": "1.3.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "start-local": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "engines": {
    "node": "^v20.5.0",
    "npm": "^9.8.0"
  },
  "build": {
    "appId": "com.info.chatty",
    "productName": "Chatty",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*",
      "RELEASE_NOTES.md",
      "package.json"
    ],
    "mac": {
      "icon": "assets/icons/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "sign": false
    },
    "generateUpdatesFilesForAllChannels": false,
    "asar": true
  },
  "dependencies": {
    "@tabler/icons-react": "^2.32.0",
    "ai": "^2.2.7",
    "html-to-image": "^1.11.11",
    "i18next": "^23.4.4",
    "js-tiktoken": "^1.0.7",
    "next": "^13.4.19",
    "next-i18next": "^14.0.0",
    "next-themes": "^0.2.1",
    "openai-edge": "^1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.1.2",
    "react-markdown": "^8.0.7",
    "react-query": "^3.39.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-mathjax": "^4.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mozilla/readability": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.15",
    "endent": "^2.1.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^22.1.0",
    "next-router-mock": "^0.9.9",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
