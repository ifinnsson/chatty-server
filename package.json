{
  "name": "chatty",
  "productName": "Chatty",
  "version": "1.3.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "start-local": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "build": {
    "appId": "com.info.chatty",
    "productName": "Chatty",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "RELEASE_NOTES.md",
      "factory-presets.json",
      "package.json"
    ],
    "mac": {
      "icon": "assets/icons/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "sign": false
    },
    "generateUpdatesFilesForAllChannels": false,
    "asar": true
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "@tabler/icons-react": "^2.29.0",
    "depcheck": "^1.4.3",
    "eventsource-parser": "^1.0.0",
    "html-to-image": "^1.11.11",
    "i18next": "^23.2.11",
    "next": "^13.4.12",
    "next-i18next": "^14.0.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.0.2",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "react-query": "^3.39.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-mathjax": "^4.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mozilla/readability": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.16",
    "@types/react-modal": "^3.16.0",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.14",
    "endent": "^2.1.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}
